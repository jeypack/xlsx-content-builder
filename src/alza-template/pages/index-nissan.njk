{% extends "layout-nissan.njk" %}
{%- block head -%}
    <title>{{advertiser}}</title>
    <meta name="ad.size" content="{{size.content}}">
{%- endblock -%}
{% block nissaninlinestyle -%}
    {% if nissan.hasFontImport -%}<style id="fontface">{%- include "partials/ads/" + dir + "/font.min.css" -%}</style>{% endif %}
    <style>{%- include "partials/ads/" + dir + "/index.min.css" -%}</style>
{%- endblock -%}
{%- block advertisertop -%}
    {% if advertiser === 'APPNEXUS' -%}
    <script type="text/javascript" src="https://acdn.adnxs.com/html5-lib/1.3.0/appnexus-html5-lib.min.js"></script>
    {% elif advertiser === 'LR' -%}
    <script src="https://cdn.qservz.com/custdata/html5/libs/lighreaction-html5-api.min.js"></script>
    <script type="text/javascript">
        var clickTag = getUrlVarsAndDecode()["clickTag"];
        function initAd() { }
    </script>
    {% elif advertiser === 'ADW' -%}
    <script type="text/javascript" src="https://tpc.googlesyndication.com/pagead/gadgets/html5/api/exitapi.js"></script>
    {% elif advertiser === 'ADF' -%}
    <script type="text/javascript">
        document.write('<script src="'+ (window.API_URL || 'https://s1.adform.net/banners/scripts/rmb/Adform.DHTML.js?bv='+ Math.random()) +'"><\/script>');
    </script>
    {% elif advertiser === 'ADFM' -%}
    <script src="mraid.js"></script>
    {% elif advertiser === 'GDN' or advertiser === 'DCM' -%}
    <script type="text/javascript">var clickTag = "https://www.google.de/";</script>
    {% endif %}
{%- endblock -%}
{%- block content -%}
    <div id="bg-exit" class="wrapper-banner">
        {%- if hasBorder -%}<div id="anim" class=""></div><div id="border-top" class="border"></div><div id="border-right" class="border"></div><div id="border-bottom" class="border"></div><div id="border-left" class="border"></div><div id="close-btn">x</div><div id="snd-on"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 352 279.2"><g><polygon points="178.1 8.7 86.6 87.9 0 87.9 0 189.7 85.3 189.7 178.1 270 178.1 8.7" style="fill:#fff"/><path d="M256.7,261.5a107.13,107.13,0,0,0-.5-114.4" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M293.8,109.5a159.68,159.68,0,0,1,.5,189.5" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M329.1,333.9a208.58,208.58,0,0,0-.5-259.2" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/></g></svg></div><div id="snd-off"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 348.1 261.3"><g><polygon points="178.1 0 86.6 79.2 0 79.2 0 181 85.3 181 178.1 261.3 178.1 0" style="fill:#fff"/><path d="M259.5,268.1,370.1,138.5" transform="translate(-32 -73.4)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M370.1,268.1,259.5,138.5" transform="translate(-32 -73.4)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/></g></svg></div>{% elif hasBorder === false -%}<div id="anim" class=""></div><div id="close-btn">x</div><div id="snd-on"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 352 279.2"><g><polygon points="178.1 8.7 86.6 87.9 0 87.9 0 189.7 85.3 189.7 178.1 270 178.1 8.7" style="fill:#fff"/><path d="M256.7,261.5a107.13,107.13,0,0,0-.5-114.4" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M293.8,109.5a159.68,159.68,0,0,1,.5,189.5" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M329.1,333.9a208.58,208.58,0,0,0-.5-259.2" transform="translate(-32 -64.7)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/></g></svg></div><div id="snd-off"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 348.1 261.3"><g><polygon points="178.1 0 86.6 79.2 0 79.2 0 181 85.3 181 178.1 261.3 178.1 0" style="fill:#fff"/><path d="M259.5,268.1,370.1,138.5" transform="translate(-32 -73.4)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/><path d="M370.1,268.1,259.5,138.5" transform="translate(-32 -73.4)" style="fill:none;stroke:#fff;stroke-linecap:round;stroke-width:20px"/></g></svg></div>{% endif %}
    </div>
    {% if advertiser === 'ADW' or advertiser === 'ADFM' or advertiser === 'GDN' or advertiser === 'DCM' -%}
    <script src="https://s0.2mdn.net/ads/studio/cached_libs/gsap_3.5.1_min.js"></script>
    {% else %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/gsap.min.js"></script>
    {% endif %}
    {% if hasMotionPath %}<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/MotionPathPlugin.min.js"></script>{%- endif -%}
    {% if hasEasePath %}<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/EasePack.min.js"></script>{% endif %}
{%- endblock -%}
{%- block bottom -%}
    {% if advertiser === 'SIZMEK' -%}
    <script src="https://secure-ds.serving-sys.com/BurstingScript/EBLoader.js"></script>
    {% elif advertiser === 'DCS' -%}
    <script src="https://s0.2mdn.net/ads/studio/Enabler.js"></script>
    {%- endif -%}
    {%- if nissan.hasAddress -%}<script>window.DYNAMIC_AD_DATA={"dealerAddress": "Autohaus Mustermann<BR>Musterstrasse 0000<Br>12345 Musterhausen"};</script>{%- endif -%}
    <script src="./egp-data.js"></script>
    {% include "partials/bottom-nissan.njk" -%}
{%- endblock %}
